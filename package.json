{
    "name": "nico-next",
    "version": "1.0.0",
    "description": "Main bot for Discord Clique",
    "main": "app.ts",
    "scripts": {
        "build": "node loadsecrets.js && etsc && cp -r src/Assets dist/src/Assets",
        "dev": "nodemon",
        "dev:debug": "set DEBUG=consola && yarn dev",
        "start": "node dist/app.js",
        "update": "npm-check -u",
        "deploy": "bash ./deploy.sh",
        "dyno:restart": "heroku scale worker=1 && heroku ps:restart worker.1 --app nico-discord-clique",
        "dyno:stop": "heroku scale worker=0",
        "prepare": "husky install",
        "prisma:update": "prisma migrate dev",
        "postinstall": "patch-package",
        "heroku-cleanup": "patch-package"
    },
    "engines": {
        "node": ">= 16.6.0"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^3.3.0",
