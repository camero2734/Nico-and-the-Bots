{
    "name": "nico-next",
    "version": "1.0.0",
    "description": "Main bot for Discord Clique server",
    "main": "app.ts",
    "scripts": {
        "dev": "nodemon",
        "dev:debug": "set DEBUG=consola && yarn dev",
        "start": "ts-node app.ts",
        "update": "npm-check -u",
        "deploy": "fish ./deploy.fish",
        "build": "rimraf dist && swc app.ts -d dist && swc src -d dist/src",
        "prepare": "husky install",
        "prisma:update": "prisma migrate dev",
        "postinstall": "patch-package",
        "docker:dev": "docker-compose up --force-recreate --build --remove-orphans",
        "docker:dev:db": "cross-env UPDATE_DB=1 yarn docker:dev",
        "docker:build": "dotenv-run-script docker:_build",
        "docker:_build": "docker build --no-cache --build-arg CRYPT64=$CRYPT64 -t camer0/nico .",
        "dokku:env": "dokku config:set nico $(sed '/^$/d' .env | tr '\\n' ' ')",
        "tasks:fetch-db": "ts-node tasks/fetch-db.ts"
    },
    "engines": {
        "node": ">= 16.6.0"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.14.0-dev.1652832619-dfe449c",
