        "@types/fuzzy-search": "^2.1.0",
        "@types/glob": "^7.2.0",
        "@types/google-spreadsheet": "^3.1.5",
        "@types/ioredis": "^4.28.8",
        "@types/mime-types": "^2.1.0",
        "@types/minio": "^7.0.13",
        "@types/node-fetch": "2.5.12",
        "@types/pako": "^1.0.1",
        "@types/ramda": "^0.27.41",
        "@types/require-all": "^3.0.2",
        "@types/seed-random": "^2.2.1",
